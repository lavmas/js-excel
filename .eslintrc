{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "arrow-parens": "off",
    "require-jsdoc": "off",
    "operator-linebreak": "off",
    "eol-last": "off",
    "indent": ["error", 4]
  },
  "extends": ["eslint:recommended", "google"]
}